{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "3.3.3",
    "@storybook/addon-links": "3.3.3",
    "@storybook/addons": "3.3.3",
    "@storybook/react-native": "3.3.3",
    "babel-eslint": "8.2.1",
    "eslint": "4.18.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.6.1",
    "eslint-plugin-react-native": "3.2.1",
    "faker": "4.1.0",
    "flow-bin": "0.66.0",
    "jest-expo": "25.1.0",
    "prettier": "1.10.2",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.1",
    "react-devtools": "^3.1.0",
    "react-dom": "16.2.0",
    "react-native-debugger-open": "^0.3.17",
    "react-native-scripts": "1.11.1",
    "react-native-storybook-loader": "1.7.0",
    "react-test-renderer": "16.2.0"
  },
  "scripts": {
    "start": "yarn run pre && react-native-scripts start",
    "start-storybook": "yarn run prestorybook && react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "yarn run pre && react-native-scripts android",
    "android-storybook":
      "yarn run prestorybook && react-native-scripts android",
    "ios": "yarn run pre && react-native-scripts ios",
    "ios-storybook": "yarn run prestorybook && react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "flow": "flow",
    "format": "prettier-eslint \"src/**/*.js\"",
    "pre":
      "cp crna-entry.js index.js && cp expo-specific-src/utils/runtimeEnvironment.js src/utils/runtimeEnvironment.js && cp expo-specific-src/components/LinearGradient.js src/components/LinearGradient.js && cp expo-specific-src/actions/appVersion.js src/actions/appVersion.js && cp expo-specific-src/reducers/appVersion.js src/reducers/appVersion.js",
    "prestorybook":
      "cp storybook-entry.js index.js && cp expo-specific-src/utils/runtimeEnvironment.js src/utils/runtimeEnvironment.js && cp expo-specific-src/components/LinearGradient.js src/components/LinearGradient.js && cp expo-specific-src/actions/appVersion.js src/actions/appVersion.js && cp expo-specific-src/reducers/appVersion.js src/reducers/appVersion.js && rnstl",
    "storybook": "storybook start -p 7007",
    "react-devtools": "react-devtools",
    "postinstall": "rndebugger-open"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|expo|glamorous-native|react-navigation)"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": ["./__stories__"],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "date-fns": "1.29.0",
    "expo": "25.0.0",
    "glamorous-native": "1.3.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-native-iphone-x-helper": "1.0.2",
    "react-navigation": "1.1.2",
    "react-navigation-redux-helpers": "1.0.1",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "twitter-text": "2.0.4",
    "uuid": "3.2.1"
  }
}
