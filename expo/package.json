{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.10",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react-native": "^3.2.10",
    "babel-eslint": "^8.0.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-native": "^3.1.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.56.0",
    "jest-expo": "21.0.2",
    "prettier": "^1.7.0",
    "prettier-eslint": "^8.1.1",
    "prettier-eslint-cli": "^4.3.2",
    "react-dom": "16.0.0",
    "react-native-scripts": "1.5.0",
    "react-native-storybook-loader": "^1.5.1",
    "react-test-renderer": "16.0.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "flow": "flow",
    "format": "prettier-eslint \"src/**/*.js\"",
    "pre":
      "cp AppExpo.js App.js && cp src/utils/environmentExpo.js src/utils/environment.js",
    "prestorybook": "cp AppStorybook.js App.js && rnstl",
    "storybook": "storybook start -p 7007"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|expo|glamorous-native|react-navigation)"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": ["./__stories__"],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "expo": "22.0.0",
    "glamorous-native": "1.2.0",
    "prop-types": "15.6.0",
    "react": "16.0.0-beta.5",
    "react-native": "0.49",
    "react-native-iphone-x-helper": "1.0.1",
    "react-navigation": "^1.0.0-beta.19"
  }
}
