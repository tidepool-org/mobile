{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "3.3.3",
    "@storybook/addon-links": "3.3.3",
    "@storybook/addons": "3.3.3",
    "@storybook/react-native": "3.3.3",
    "babel-eslint": "8.1.2",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "4.14.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.4.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-react-native": "3.2.0",
    "faker": "4.1.0",
    "flow-bin": "0.62.0",
    "prettier": "1.9.2",
    "prettier-eslint": "8.7.0",
    "prettier-eslint-cli": "4.7.0",
    "react-dom": "16.0.0",
    "react-native-storybook-loader": "1.7.0",
    "react-test-renderer": "16.0.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "flow": "flow",
    "format": "prettier-eslint \"src/**/*.js\"",
    "pre":
      "rm -fR src && cp -fR ../expo/src/ src && rm -fR assets && cp -fR ../expo/assets/ assets && cp react-native-specific-src/utils/runtimeEnvironment.js src/utils/runtimeEnvironment.js && cp react-native-specific-src/components/LinearGradient.js src/components/LinearGradient.js && cp react-native-specific-src/actions/appVersion.js src/actions/appVersion.js && cp react-native-specific-src/reducers/appVersion.js src/reducers/appVersion.js"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|expo|glamorous-native)"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": ["./src"],
      "pattern": "**/__stories__/*.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "date-fns": "1.29.0",
    "glamorous-native": "1.3.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-native-iphone-x-helper": "1.0.2",
    "react-native-linear-gradient": "2.4.0",
    "react-native-version-number": "0.3.1",
    "react-navigation": "1.1.1",
    "react-navigation-redux-helpers": "1.0.1",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "twitter-text": "2.0.4",
    "uuid": "3.2.1"
  },
  "rnpm": {
    "assets": ["./assets/fonts"]
  }
}
