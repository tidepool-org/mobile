{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "pre": "cp crna-entry.js index.js",
    "prestorybook": "cp storybook-entry.js index.js && rnstl",
    "test": "node node_modules/jest/bin/jest.js",
    "format": "prettier-eslint \"src/**/*.js\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "axios": "0.19.0",
    "base-64": "0.1.0",
    "date-fns": "1.30.1",
    "expo": "34.0.4",
    "expo-asset": "6.0.0",
    "expo-constants": "6.0.0",
    "expo-file-system": "6.0.2",
    "expo-font": "6.0.1",
    "expo-gl": "6.0.0",
    "expo-keep-awake": "6.0.0",
    "expo-linear-gradient": "6.0.0",
    "expo-three": "4.0.4",
    "expokit": "34.0.2",
    "glamorous-native": "1.4.2",
    "native-base": "^2.12.1",
    "prop-types": "15.7.2",
    "formik": "^1.5.8",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.1.tar.gz",
    "react-native-device-info": "2.3.1",
    "react-native-local-mongodb": "2.2.9",
    "react-native-walkthrough-tooltip": "0.5.3",
    "react-navigation": "2.0.4",
    "react-navigation-redux-helpers": "1.1.2",
    "react-native-datepicker": "^1.7.2",
    "react-native-hr-component": "^2.0.2",
    "react-native-picker-select": "^6.3.0",
    "react-native-switch": "^1.5.0",
    "react-redux": "6.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "rollbar-react-native": "0.5.0",
    "three": "0.107.0",
    "three-bmfont-text": "2.3.0",
    "twitter-text": "3.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "5.1.11",
    "@storybook/addon-ondevice-knobs": "5.1.11",
    "@storybook/addon-storyshots": "5.1.11",
    "@storybook/addons": "5.1.11",
    "@storybook/react-native": "5.1.11",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-preset-expo": "6.0.0",
    "babel-runtime": "6.26.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-native": "3.7.0",
    "expo-cli": "3.0.9",
    "faker": "4.1.0",
    "flow-bin": "0.105.2",
    "jest-expo": "34.0.0",
    "patch-package": "6.1.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-devtools": "3.6.3",
    "react-dom": "16.8.6",
    "react-native-debugger-open": "0.3.19",
    "react-native-gesture-handler": "1.3.0",
    "react-native-storybook-loader": "1.8.0",
    "react-native-unimodules": "0.5.4",
    "react-test-renderer": "16.8.3"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./__stories__"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "setupFilesAfterEnv": [
      "./jest-setupTestFramework.js"
    ]
  },
  "private": true
}
