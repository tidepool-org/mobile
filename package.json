{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "yarn expo run:android",
    "ios": "yarn expo run:ios",
    "eject": "npx expo prebuild",
    "pre": "cp crna-entry.js index.js",
    "prestorybook": "rnstl",
    "test": "node node_modules/jest/bin/jest.js",
    "format": "prettier-eslint \"src/**/*.js\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@babel/preset-env": "7.21.4",
    "@emotion/core": "11.0.0",
    "@react-navigation/core": "^6.4.8",
    "@storybook/api": "7.0.7",
    "@storybook/components": "7.0.7",
    "@storybook/core-events": "7.0.7",
    "@storybook/theming": "7.0.7",
    "axios": "1.3.6",
    "base-64": "1.0.0",
    "date-fns": "2.29.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "expo": "~48.0.11",
    "expo-asset": "~8.9.1",
    "expo-constants": "~14.2.1",
    "expo-file-system": "~15.2.2",
    "expo-font": "~11.1.1",
    "expo-gl": "~12.4.0",
    "expo-keep-awake": "~12.0.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-mail-composer": "~12.1.1",
    "expo-modules-autolinking": "^1.2.0",
    "expo-modules-core": "^1.2.6",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-three": "7.0.0",
    "expo-updates": "~0.16.4",
    "glamorous-native": "2.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.6",
    "react-native-device-info": "10.6.0",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-local-mongodb": "4.0.0",
    "react-native-reanimated": "~3.1.0",
    "react-native-screens": "~3.20.0",
    "react-native-walkthrough-tooltip": "1.5.0",
    "react-native-web": "~0.19.4",
    "react-navigation": "4.4.4",
    "react-navigation-redux-helpers": "4.0.1",
    "react-redux": "8.0.5",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "rollbar-react-native": "0.9.3",
    "three": "0.151.3",
    "three-bmfont-text": "3.0.1",
    "twitter-text": "3.1.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/runtime": "7.21.0",
    "@rnx-kit/align-deps": "2.2.2",
    "@storybook/addon-controls": "^7.0.7",
    "@storybook/addon-ondevice-knobs": "6.5.3",
    "@storybook/addon-storyshots": "7.0.7",
    "@storybook/addons": "7.0.7",
    "@storybook/react-native": "6.5.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.5.0",
    "babel-preset-expo": "^9.3.0",
    "eslint": "8.39.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-native": "4.0.0",
    "faker": "6.6.6",
    "flow-bin": "0.204.0",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.0",
    "metro": "^0.76.3",
    "patch-package": "6.5.1",
    "postinstall-prepare": "2.0.0",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "prettier-eslint-cli": "7.1.0",
    "react-devtools": "4.27.6",
    "react-native-debugger-open": "0.3.25",
    "react-native-storybook-loader": "2.0.5",
    "react-test-renderer": "18.2.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./__stories__"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "setupFilesAfterEnv": [
      "./jest-setupTestFramework.js"
    ]
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.71"
      ],
      "capabilities": [
        "animation",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "react",
        "react-dom",
        "react-test-renderer",
        "screens"
      ]
    }
  }
}
