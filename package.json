{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "npx expo prebuild",
    "pre": "cp crna-entry.js index.js",
    "prestorybook": "cp storybook-entry.js index.js && rnstl",
    "test": "node node_modules/jest/bin/jest.js",
    "format": "prettier-eslint \"src/**/*.js\"",
    "postinstall": "patch-package",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
  "dependencies": {
    "@react-three/fiber": "^8.13.0",
    "axios": "0.19.0",
    "base-64": "0.1.0",
    "date-fns": "1.30.1",
    "expo": "~46.0.21",
    "expo-asset": "~8.6.3",
    "expo-constants": "~13.2.4",
    "expo-dev-client": "~1.3.1",
    "expo-file-system": "~14.1.0",
    "expo-font": "~10.2.0",
    "expo-gl": "~11.4.0",
    "expo-keep-awake": "~10.2.0",
    "expo-linear-gradient": "~11.4.0",
    "expo-mail-composer": "~11.3.0",
    "expo-three": "4.0.4",
    "expokit": "34.0.2",
    "glamorous-native": "1.4.2",
    "prop-types": "15.7.2",
    "react": "18.0.0",
    "react-dom": "^18.2.0",
    "react-native": "0.69.9",
    "react-native-device-info": "2.3.1",
    "react-native-local-mongodb": "2.2.9",
    "react-native-walkthrough-tooltip": "0.5.3",
    "react-native-web": "~0.18.7",
    "react-navigation": "2.0.4",
    "react-navigation-redux-helpers": "1.1.2",
    "react-redux": "6.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "rollbar-react-native": "0.5.0",
    "three": "^0.152.2",
    "three-bmfont-text": "2.3.0",
    "twitter-text": "3.0.0",
    "uuid": "3.3.2",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "react-dom": "18.0.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@storybook/addon-actions": "6.5.0",
    "@storybook/addon-controls": "6.5.0",
    "@storybook/addon-ondevice-actions": "^6.5.3",
    "@storybook/addon-storyshots": "6.5.0",
    "@storybook/addons": "6.5.0",
    "@storybook/cli": "^7.0.13",
    "@storybook/core-common": "6.5.0",
    "@storybook/react-native": "6.5.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-preset-expo": "~9.2.0",
    "babel-runtime": "6.26.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-native": "3.7.0",
    "faker": "4.1.0",
    "flow-bin": "0.105.2",
    "jest-expo": "^46.0.0",
    "patch-package": "6.1.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-devtools": "3.6.3",
    "react-dom": "18.0.0",
    "react-native-debugger-open": "0.3.19",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-storybook-loader": "1.8.0",
    "react-test-renderer": "16.8.3"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./__stories__"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "setupFilesAfterEnv": [
      "./jest-setupTestFramework.js"
    ]
  }
}
