{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "3.4.6",
    "@storybook/addon-knobs": "^3.4.6",
    "@storybook/addon-links": "3.4.6",
    "@storybook/addon-storyshots": "3.4.6",
    "@storybook/addons": "3.4.6",
    "@storybook/react-native": "3.4.6",
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.9.0",
    "eslint-plugin-react-native": "3.2.1",
    "faker": "4.1.0",
    "flow-bin": "0.73.0",
    "jest-expo": "^27.0.1",
    "prettier": "1.13.4",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.1",
    "react-devtools": "3.2.3",
    "react-dom": "16.3.2",
    "react-native-debugger-open": "^0.3.17",
    "react-native-storybook-loader": "1.8.0",
    "react-test-renderer": "16.3.2"
  },
  "scripts": {
    "test": "node node_modules/jest/bin/jest.js --watch",
    "flow": "flow",
    "format": "prettier-eslint \"src/**/*.js\"",
    "pre": "cp crna-entry.js index.js",
    "prestorybook": "cp storybook-entry.js index.js && rnstl",
    "storybook": "storybook start -p 7007",
    "react-devtools": "react-devtools",
    "postinstall": "rndebugger-open"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|expo|react-navigation|glamorous-native)"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "setupTestFrameworkScriptFile": "./jest-setupTestFramework.js"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./__stories__"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "date-fns": "1.29.0",
    "expo": "27.0.2",
    "expo-three": "^2.2.2-alpha.1",
    "glamorous-native": "1.4.0",
    "prop-types": "15.6.1",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-27.0.2.tar.gz",
    "react-native-device-info": "^0.21.5",
    "react-navigation": "2.0.4",
    "react-navigation-redux-helpers": "1.1.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "three": "0.92.0",
    "three-bmfont-text": "^2.3.0",
    "twitter-text": "2.0.4",
    "uuid": "3.2.1"
  }
}
