{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "eject": "expo eject",
    "pre": "cp crna-entry.js index.js",
    "prestorybook": "cp storybook-entry.js index.js && rnstl",
    "test": "node node_modules/jest/bin/jest.js",
    "format": "prettier-eslint \"src/**/*.js\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "axios": "0.19.0",
    "base-64": "0.1.0",
    "date-fns": "1.30.1",
    "expo": "^42.0.0",
    "expo-asset": "~8.3.3",
    "expo-constants": "~11.0.1",
    "expo-file-system": "~11.1.3",
    "expo-font": "~9.2.1",
    "expo-gl": "~10.4.2",
    "expo-keep-awake": "~9.2.0",
    "expo-linear-gradient": "~9.2.0",
    "expo-mail-composer": "~10.2.2",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-three": "4.0.4",
    "expo-updates": "~0.8.2",
    "glamorous-native": "1.4.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.0",
    "react-native": "0.64.3",
    "react-native-device-info": "2.3.1",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-local-mongodb": "2.2.9",
    "react-native-reanimated": "~2.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-unimodules": "~0.14.5",
    "react-native-walkthrough-tooltip": "0.5.3",
    "react-native-web": "~0.13.12",
    "react-navigation": "4.4.1",
    "react-navigation-redux-helpers": "1.1.2",
    "react-redux": "6.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "rollbar-react-native": "0.5.0",
    "three": "0.107.0",
    "three-bmfont-text": "2.3.0",
    "twitter-text": "3.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/runtime": "7.6.2",
    "@rnx-kit/align-deps": "2.2.2",
    "@storybook/addon-knobs": "5.1.11",
    "@storybook/addon-ondevice-knobs": "5.1.11",
    "@storybook/addon-storyshots": "5.1.11",
    "@storybook/addons": "5.1.11",
    "@storybook/react-native": "5.1.11",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.9.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-native": "3.7.0",
    "expo-cli": "3.0.10",
    "faker": "4.1.0",
    "flow-bin": "0.105.2",
    "jest": "24.8.0",
    "jest-expo": "^42.0.1",
    "patch-package": "6.1.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-eslint-cli": "5.0.0",
    "react-devtools": "3.6.3",
    "react-native-debugger-open": "0.3.19",
    "react-native-storybook-loader": "1.8.0",
    "react-native-unimodules": "~0.8.1",
    "react-test-renderer": "16.11.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./__stories__"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "setupFilesAfterEnv": [
      "./jest-setupTestFramework.js"
    ]
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.62"
      ],
      "capabilities": [
        "animation",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "react",
        "react-dom",
        "react-test-renderer",
        "screens"
      ]
    }
  }
}
